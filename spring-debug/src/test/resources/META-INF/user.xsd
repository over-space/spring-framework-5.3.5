<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://www.learning.org/schema/user"
            elementFormDefault="qualified">
    
    <element name="user">
        <complexType>
            <attribute name="id" type="string"></attribute>
            <attribute name="username" type="string"></attribute>
            <attribute name="password" type="string"></attribute>
            <attribute name="email" type="string"></attribute>
            <attribute name="age" type="integer"></attribute>
        </complexType>
    </element>

    
</schema>


